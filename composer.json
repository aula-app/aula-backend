{
    "require": {
        "auth0/auth0-php": "^8.11",
        "nyholm/psr7": "^1.8",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "classmap": [
            "classes/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
